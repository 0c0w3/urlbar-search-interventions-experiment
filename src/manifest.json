{
  "manifest_version": 2,
  "name": "Urlbar Search Interventions Experiment",
  "version": "1.0.0",
  "description": "Shows relevant tips in the urlbar view when you type certain search terms.",
  "applications": {
    "gecko": {
      "id": "urlbar-search-interventions-experiment@shield.mozilla.org",
      "strict_min_version": "70"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "urlbar-search-interventions-experiment@shield.mozilla.org",
      "strict_min_version": "70"
    }
  },
  "permissions": [
    "normandyAddonStudy",
    "urlbar"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "incognito": "spanning",
  "hidden": true
}
